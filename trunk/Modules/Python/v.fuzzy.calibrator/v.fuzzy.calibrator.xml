<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0"
xmlns:ows="http://www.opengis.net/ows/1.1"
xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd"
 service="WPS" version="1.0.0" xml:lang="en-US"> 
	<ProcessDescription wps:processVersion="1" storeSupported="true" statusSupported="true">
		<ows:Identifier>v.fuzzy.calibrator</ows:Identifier>
		<ows:Title>Calibrate a fuzzy inference model parameter based on vector data</ows:Title>
		<ows:Abstract>http://grass.osgeo.org/grass70/manuals/html70_user/v.fuzzy.calibrator.html</ows:Abstract>
		<ows:Metadata xlink:title="vector" />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>input</ows:Identifier>
				<ows:Title>Data source for direct OGR access</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>type</ows:Identifier>
				<ows:Title>Feature type</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>point</ows:Value>
						<ows:Value>centroid</ows:Value>
						<ows:Value>area</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>point</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>factors</ows:Identifier>
				<ows:Title>Names of the table columns of the fuzzy factors</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>target</ows:Identifier>
				<ows:Title>Name of the table column of the target variable</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>initparameter</ows:Identifier>
				<ows:Title>The name of the initial XML (weighted) fuzzy inference parameter file, including factors which are already calibrated.</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>weightfactor</ows:Identifier>
				<ows:Title>Name of the table column of the weighting variable</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>iterations</ows:Identifier>
				<ows:Title>The maximum number of iterations</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer">integer</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>5000</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1024">
				<ows:Identifier>fuzzysets</ows:Identifier>
				<ows:Title>The number of fuzzy sets to be used for calibration for each factor: in case of 4 factors each 2 fuzzy sets: 2,2,2,2</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer">integer</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>sd</ows:Identifier>
				<ows:Title>The standard deviation used to modify the fuzzy inference model</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>breakcrit</ows:Identifier>
				<ows:Title>The break criteria</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>0.01</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>weightnum</ows:Identifier>
				<ows:Title>The number of weights used for calibration</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer">integer</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>6</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>null</ows:Identifier>
				<ows:Title>The value used fo no data</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>9999</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>treduce</ows:Identifier>
				<ows:Title>This factor is used to reduce the annealing temperature each step</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1.01</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>sdreduce</ows:Identifier>
				<ows:Title>This factor is used to reduce the standard deviation each step</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1.01</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-b</ows:Identifier>
				<ows:Title>Use boostrap aggregation (bagging) for input data selection</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-w</ows:Identifier>
				<ows:Title>Use weighting for input data calibration. A weightingfactor and the number of weights must be provided.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>log</ows:Identifier>
				<ows:Title>The name of the logfile to store the model error and AKAIKE criteria</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>parameter</ows:Identifier>
				<ows:Title>Output name of the calibrated XML (weighted) fuzzy inference parameter file</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>output</ows:Identifier>
				<ows:Title>The best fitted model result as vector map</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
			<Output>
				<ows:Identifier>vtkoutput</ows:Identifier>
				<ows:Title>The best fitted model result as VTK XML poly data output (.vtp added)</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
