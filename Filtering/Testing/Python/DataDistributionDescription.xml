<tag2e:DataDistributionDescription  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns:tag2e='http://tag2e.googlecode.com/files/DataDistributionDescription'
   xsi:schemaLocation='http://tag2e.googlecode.com/files/DataDistributionDescription DataDistributionDescription.xsd' 
   name="test">
    <tag2e:Variable name="pH" type="norm">
        <tag2e:Norm mean="6.5" sd="0.4"/>
    </tag2e:Variable>
    <tag2e:Variable name="temp" type="norm">
        <tag2e:Norm mean="8" sd="2"/>
    </tag2e:Variable>
    <tag2e:Variable name="nrate" type="lnorm">
        <tag2e:Lnorm meanlog="3" sdlog="0.3"/>
    </tag2e:Variable>
    <tag2e:Variable name="sand" type="binom">
        <tag2e:Binom size="12" prob="1"/>
    </tag2e:Variable>
    <tag2e:Variable name="prec" type="chisq">
        <tag2e:Chisq df="80.0" ncp="0.0"/>
    </tag2e:Variable>
</tag2e:DataDistributionDescription>
